# Example backend .env
# Copy this file to `backend/.env` and populate with your credentials. Do NOT commit secrets.

# Supabase project URL (include https://)
SUPABASE_URL=https://your-project-ref.supabase.co

# Supabase service role key (keep secret)
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Optional: if you want the API to query a specific table
DATA_TABLE=distribuicao_raw
